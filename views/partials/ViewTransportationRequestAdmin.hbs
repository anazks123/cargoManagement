<html>
    <head>

    </head>
    <body>
        <div class="jumbotron">
             <table class="table table-borderless" id="example4">
            <thead>
                <tr>
                <th scope="col">Customer Name</th>
                <th scope="col">Email</th>
                <th scope="col">Number of containers</th>
                <th scope="col">City</th>
               
                
                    <th scope="col">Shipment Type</th>
                   
                     <th scope="col">Action</th>
                      
                </tr>
            </thead>
            <tbody>
                {{# each transportations}}
                <tr>
                
                        <td>{{this.CustomerId.CustomerName}}</td>
                        <td>{{this.CustomerId.email}}</td>
                        <td>{{this.NumberContainer}}</td>
                         <td>{{this.city}}</td>
                         <td>{{this.ShipmentType}}</td>
                        
                         {{!-- <td><a href="/delete/ShipMaster/{{this._id}}"  style="margin-top:10px;"><i class="fa fa-toggle-off" aria-hidden="true" style="color:#557a95"></i></a></td> --}}
                          {{!-- <td><a href="/Update/{{this._id}}"  style="margin-top:10px;"><i class="fa fa-toggle-on" aria-hidden="true"  style="color:#557a95" ></i></a></td> --}}

                          {{# if this.Status}}
										<td><a href="/update/transportationFalse/{{this.Status}}/{{this._id}}/{{this.CustomerId.email}}/{{this.ShipmentType}}/{{this.NumberContainer}}"><i class="fa fa-toggle-on" aria-hidden="true"></i></a></td>
										{{else}}
										<td><a href="/update/transportationTrue/{{this.Status}}/{{this._id}}/{{this.CustomerId.email}}/{{this.ShipmentType}}/{{this.NumberContainer}}"><i class="fa fa-toggle-off" aria-hidden="true"></i></a></td>
										{{/if}}
                </tr>
                {{/each}}
               
            </tbody>
            </table>
        </div>
    </body>
</html>