<table class="table table-borderless" id="example7">
            <thead>
                <tr>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Gender</th>
                <th scope="col">City</th>
               
                
                    <th scope="col">Contact Number</th>
                    <th scope="col">Registered Date</th>
                   
                </tr>
            </thead>
            <tbody>
                {{# each Customers}}
                <tr>
                
                        <td>{{this.CustomerName}}</td>
                        <td>{{this.email}}</td>
                        <td>{{this.gender}}</td>
                         <td>{{this.city}}</td>
                         <td>{{this.Contact}}</td>
                          <td>{{this.regDate}}</td>
                       
                       
                </tr>
                {{/each}}
               
            </tbody>
            </table>


           